<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .movie-label {
            width: 100%;
        }

        .movie-label:hover {
            text-decoration: line-through;
            color: #999999;
            cursor: pointer;
        }
    </ui:style>
    <g:Label ui:field="title" addStyleNames="{style.movie-label}"/>
</ui:UiBinder>